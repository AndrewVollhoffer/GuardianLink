<div class="card">
  <div class="card-body">
    <h3 class="card-header text-center"><%= @user.first_name %></h5>
    <% if !user.admin? and user.id <= 12 %>
      <h3 class="badge rounded-pill text-bg-success">Example</h3>
    <% end %>
    <h5 class="card-title text-center">Areas of Concern</h5>
    <p class="card-text text-center"> <%= @user.bio %></p>
  </div>
</div>
